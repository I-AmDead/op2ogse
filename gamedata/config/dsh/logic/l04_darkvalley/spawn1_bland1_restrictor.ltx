; Рестриктор появления Бленда

[logic]
active = sr_idle

[sr_idle]
on_actor_inside = {+bland_flash_start} sr_idle@time %+spawn_bland =arhara_dialog.bland_sms%

[sr_idle@time]
on_timer = 500 | nil %+lapti_samoskoki =arhara_dialog.dell_spawn_bland_restr =release%

[sr_idle@nil]
on_info = nil %=release%
