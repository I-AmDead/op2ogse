[logic]
active = sr_idle

[sr_idle]
on_actor_inside = {+nayti_taynik_radista_done} nil %=arhara_dialog.seif_zakrut_sms%
single = true

[sr_idle@time]
on_timer = 25000 | sr_idle@time1 %=arhara_dialog.seif_zakrut_sms%
single = true

[sr_idle@time1]
on_timer = 8000 | sr_idle@time2 %=arhara_dialog.delete_kluk_and_radist =arhara_dialog.seif_zakrut_sms1%
single = true

[sr_idle@time2]
on_timer = 20000 | sr_idle@nil %+spawn_kluk_na_pripyti =arhara_dialog.spawn_gig_sadik%
single = true

[sr_idle@nil]
on_info = nil
