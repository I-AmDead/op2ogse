dsh_change_profile_on_empty = true
dsh_profiles = enemies,70,friends,30
dsh_walking  = true
dsh_population_locked = true

[dsh.gulag.override.killer_lager_upiter.enemies]:dsh.gulag.override.killer_lager_upiter

