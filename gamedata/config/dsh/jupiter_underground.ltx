; -*- mode: conf-windows; coding: windows-1251-dos -*-

[dsh.gulag.idle.jupiter_underground]:dsh.gulag.idle
monster = 12, 24


[dsh.se_respawn.podzemka_karlushy_respawn]
respawn_section = fracture_weak, fracture_normal, fracture_strong, bloodsucker_strong


[dsh.se_respawn.podzemka_polterburer_respawn]
respawn_section = burer_weak, burer_normal, burer_strong


[dsh.se_respawn.podzemka_karliki_respawn]
respawn_section = tushkano_strong


[dsh.gulag.override.podzemka_karlushy]
type        = general_lair
communities = fracture, bloodsucker
dsh_change_profile_on_empty = true
dsh_paths.mob_home = home
dsh_profiles = bloodsuckers, fractures, snorks, tushkano
dsh_walking  = true

[dsh.gulag.override.podzemka_karlushy.bloodsuckers]:dsh.gulag.override.podzemka_karlushy
capacity    = 5
communities = bloodsucker

[dsh.gulag.override.podzemka_karlushy.fractures]:dsh.gulag.override.podzemka_karlushy
communities = fracture

[dsh.gulag.override.podzemka_karlushy.snorks]:dsh.gulag.override.podzemka_karlushy
communities = snork

[dsh.gulag.override.podzemka_karlushy.tushkano]:dsh.gulag.override.podzemka_karlushy
communities = tushkano

[dsh.gulag.idle.podzemka_karlushy]:dsh.gulag.idle.jupiter_underground


