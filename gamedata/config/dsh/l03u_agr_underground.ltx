; -*- mode: conf-windows; coding: windows-1251-dos -*-

; бандиты в подземке Агропрома
[dsh.gulag.override.agr_u_bandits]
communities = bandit
dsh_change_profile_on_empty = true
dsh_profiles = def,40,def2,30,def3,30
dsh_walking  = true
dsh_population_locked = true

[dsh.gulag.override.agr_u_bandits.def]:dsh.gulag.override.agr_u_bandits

[dsh.gulag.override.agr_u_bandits.def2]:dsh.gulag.override.agr_u_bandits
communities = killer

[dsh.gulag.override.agr_u_bandits.def3]:dsh.gulag.override.agr_u_bandits
communities = sniper


; Военные в подземке Агропрома. Будут до тех пор, пока там не появятся
; кровососы.
[dsh.gulag.override.agr_u_soldiers]
cond = {-agroprom_military_case_done}
