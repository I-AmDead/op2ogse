; Бандиты в церкви
[dsh.gulag.override.marsh_church_band]
dsh_after_delay = 72, 168 ;от трех суток до недели
dsh_change_profile_on_empty = true
dsh_profiles    = bandits,40,bandits2,30,bandits3,30
dsh_walking     = true
dsh_population_locked = true

[dsh.gulag.override.marsh_church_band.bandits]:dsh.gulag.override.marsh_church_band

[dsh.gulag.override.marsh_church_band.bandits2]:dsh.gulag.override.marsh_church_band
communities = killer

[dsh.gulag.override.marsh_church_band.bandits3]:dsh.gulag.override.marsh_church_band
communities = sniper


; Митя
[dsh.gulag.override.mitya_marsh]
cond       = {-klyak_forest_doc_done}
on_cleanup = dsh.free_mitya

; и его коллеги
[dsh.gulag.override.grab_mitya_marsh]
cond       = {-klyak_forest_doc_scene -klyak_forest_doc_next}
on_cleanup = dsh.free_grab_mitya_marsh


