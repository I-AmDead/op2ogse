; -*- mode: conf-windows; coding: windows-1251-dos -*-

; за мостом на пути от мех. двора к водокачке
[dsh.gulag.override.poltergeist_marsh]
communities = flesh, pseudo_gigant ;flesh, giant


; сталкеры Александрыча, на Болотах
[dsh.gulag.override.stalker_green1]
communities = stalker, green, nebo, freedom
dsh_after_delay = 72, 168 ;от трех суток до недели
dsh_change_profile_on_empty = true
dsh_profiles    = stalkers
dsh_walking     = true
dsh_population_locked = true

[dsh.gulag.override.stalker_green1.stalkers]:dsh.gulag.override.stalker_green1


; Сталкеры на Болотах, недалеко от водокачки
[dsh.gulag.override.stalker_green2]
communities = stalker, green, nebo, freedom
dsh_walking = true
dsh_after_delay = 72, 168 ;от трех суток до недели
dsh_change_profile_on_empty = true
dsh_profiles    = stalkers
dsh_walking     = true
dsh_population_locked = true

[dsh.gulag.override.stalker_green2.stalkers]:dsh.gulag.override.stalker_green2

[dsh.xr_kamp.stalker_green2_centr_two]
level_vertex_id = 199581


; Мстители на Болотах, на хуторе у перехода на Кордон
[dsh.gulag.override.marsh_sniper]
