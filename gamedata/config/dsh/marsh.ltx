; Сталкеры Калмыка, на Болотах
[dsh.gulag.override.kalmyak_marsh]
communities = stalker, green, nebo, freedom
dsh_walking = true


; пост чистого неба, перед телепортом на базу
[dsh.gulag.override.marsh_small_nebo]
dsh_walking = true
dsh_population_locked = true


; группа Дьяка
[dsh.gulag.override.marsh_exit_nebo]
