; Охотники Дэна
[dsh.gulag.override.marsh_hunters]
dsh_walking = true


; коты между охотниками и мстителями
[dsh.gulag.override.marsh_cat]
dsh_walking = true


; контроллер на хуторе
[dsh.gulag.override.marsh_controller_hutor]
dsh_walking = true

; и его зомби
[dsh.gulag.override.marsh_zombi_hutor]
dsh_walking = true


; Изломы и библиотекари за мостиком. Откроется после флешки Шустрого, что бы
